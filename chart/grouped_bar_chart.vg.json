{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width" : 1000,
    "height" : 400,
    "data" : {"url" : "https://raw.githubusercontent.com/xlin0041/3179/main/Worldwide%20Vaccine%20Data.csv"},
    "repeat": {"layer": ["% of population vaccinated","% of population fully vaccinated"]},
    "spec" : {
      "mark" : {
        "type" : "bar",
        "tooltip" : true
      },
      "encoding": {
        "x" : {
          "field" : "Country",
          "type" : "nominal"
        },
        "y" : {
          "field" : {"repeat": "layer"},
          "type" : "quantitative",
          "title" : "Percantage of Population"
        },
        "color" : {"datum" : {"repeat" : "layer"}, "title" : "Title"},
        "xOffset" : {"datum" : {"repeat" : "layer"}}
      }
    }
    
  }